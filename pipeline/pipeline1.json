				},
				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": "INSERT INTO silver_olist_geolocation_dataset\nSELECT DISTINCT\n    RIGHT('00000' + CONVERT(VARCHAR, geolocation_zip_code_prefix), 5) AS geolocation_zip_code_prefix,\n    REPLACE(REPLACE(REPLACE(geolocation_city, 'Â´', ''), '.', ''), '*', '') AS geolocation_city,\n    geolocation_state\nFROM bronze_olist_geolocation_dataset\nORDER BY geolocation_city;"
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			},
